<h3>Create a new article</h3>
<%= form_for @news do |f| %>
<%= f.label :title, 'News title' %>
<%= f.text_field :title %>
<%= f.label  :content, 'News content' %>
<%= f.cktext_area :content, class: 'ckeditor' %>
<%= f.label :file %>
<%= f.file_field :file %>
<%= f.submit 'Submit', class: 'button inc' %>
<% end %>
